    },
    "dependencies": {},
    "devDependencies": {
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "@eslint/js": "^9.21.0",
        "@types/node": "^22.10.2",
        "eslint": "^9.21.0",
        "globals": "^16.0.0",
        "tsc-alias": "^1.8.16",
        "typescript-eslint": "^8.24.1"
    }
}
